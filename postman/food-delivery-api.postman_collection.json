                  "raw": "{\n    \"user_id\": \"{{user_id}}\",\n    \"order_id\": \"{{order_id}}\",\n    \"restaurant_rating\": 5,\n    \"delivery_agent_rating\": 5,\n    \"restaurant_review\": \"Excellent burger, hot and delicious!\",\n    \"delivery_review\": \"Fast delivery, very professional agent.\"\n}"
                },
                "url": {
                  "raw": "{{user_service_url}}/api/ratings",
                  "host": ["{{user_service_url}}"],
                  "path": ["api", "ratings"]
                }
              }
            }
          ]
        }
      ]
    }
  ]
}